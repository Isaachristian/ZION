{
	"name": "maca",
	"version": "0.1.0",
	"description": "Multi-user Application Control Arbitor",
	"main": "src/index.ts",
	"scripts": {
		"dev": "nodemon --exec \"node --experimental-strip-types --no-warnings\" src/index.ts --listen-port=8081 --destination-port=5472 --destination-host=localhost --https=false --loglevel=verbose",
