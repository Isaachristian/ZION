	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Isaachristian/MACA/issues"
	},
	"homepage": "https://github.com/Isaachristian/MACA#readme",
	"devDependencies": {
		"@biomejs/biome": "^2.0.0",
		"@types/node": "^22.15.31",
		"nodemon": "^3.1.10",
		"typescript": "^5.8.3"
	}
